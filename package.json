{
  "name": "racko",
  "version": "0.0.1",
  "description": "Rack-O",
  "author": "Callum Jefferies",
  "scripts": {
    "test-lint": "eslint test/**/*Test.js",
    "test": "npm run test-lint && browserify test/**/*Test.js | testling",
    "clean": "rm -rf build/*",
    "start": "npm run clean && FIREBASE=https://dazzling-heat-6913.firebaseio.com/ watchify src/js/index.js -o build/bundle.js",
    "build": "npm run clean && FIREBASE=https://dazzling-heat-6913.firebaseio.com/ browserify src/js/index.js -o build/bundle.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "dependencies": {
    "array-next": "0.0.1",
    "classnames": "^1.2.1",
    "deck": "0.0.4",
    "firebase": "^2.2.4",
    "flux": "^2.0.1",
    "immutable": "^3.7.2",
    "keymirror": "^0.1.1",
    "node-sass": "^2.1.1",
    "range": "0.0.2",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel-core": "^5.1.10",
    "babel-eslint": "^3.0.1",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.1.10",
    "babel-tape-runner": "^1.1.0",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "envify": "^3.4.0",
    "eslint": "^0.19.0",
    "eslint-plugin-react": "^2.1.1",
    "tape": "^4.0.0",
    "testling": "^1.7.1",
    "watchify": "^3.2.0"
  }
}
