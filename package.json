{
  "name": "racko",
  "version": "0.0.1",
  "description": "Rack-O",
  "author": "Callum Jefferies",
  "scripts": {
    "gulp": "eslint gulpfile.js && babel-node ./node_modules/.bin/gulp",
    "lint": "eslint src/**/*.js",
    "test-lint": "eslint test/**/*Test.js",
    "test": "npm run test-lint && browserify -t babelify test/**/*Test.js | tape-run",
    "test-node": "npm run test-lint && babel-tape-runner test/**/*Test.js",
    "start": "npm run watch",
    "build": "npm run lint && npm run gulp build",
    "watch": "npm run lint && npm run gulp watch",
    "deploy": "NODE_ENV=production npm run build && firebase deploy && rm -rf public"
  },
  "dependencies": {
    "array-next": "0.0.1",
    "classnames": "^1.2.1",
    "deck": "0.0.4",
    "firebase": "^2.2.4",
    "flux": "^2.0.1",
    "human-time": "0.0.1",
    "immutable": "^3.7.2",
    "keymirror": "^0.1.1",
    "range": "0.0.2",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel": "^5.2.16",
    "babel-core": "^5.1.10",
    "babel-eslint": "^3.0.1",
    "babel-runtime": "^5.1.10",
    "babel-tape-runner": "^1.1.0",
    "babelify": "^6.0.2",
    "browserify": "^10.1.2",
    "del": "^1.1.1",
    "envify": "^3.4.0",
    "eslint": "^0.19.0",
    "eslint-plugin-react": "^2.1.1",
    "firebase-tools": "^1.1.6",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "tape": "^4.0.0",
    "tape-run": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1"
  }
}
