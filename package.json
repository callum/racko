{
  "name": "racko",
  "version": "0.0.1",
  "description": "Rack-O",
  "author": "Callum Jefferies",
  "scripts": {
    "pretest": "eslint test/**/*Test.js",
    "test": "browserify -t babelify test/**/*Test.js | testling",
    "start": "http-server build",
    "clean": "rm -rf build/*",
    "copy": "cp src/index.html build",
    "lint": "eslint src/**/*.js",
    "setup": "npm run clean && npm run copy && npm run lint",
    "watch": "npm run setup && babel-node bin/watch.js",
    "build": "npm run setup && babel-node bin/build.js"
  },
  "dependencies": {
    "array-next": "0.0.1",
    "classnames": "^1.2.1",
    "deck": "0.0.4",
    "firebase": "^2.2.4",
    "flux": "^2.0.1",
    "immutable": "^3.7.2",
    "keymirror": "^0.1.1",
    "node-sass": "^2.1.1",
    "range": "0.0.2",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel-core": "^5.1.10",
    "babel-eslint": "^3.0.1",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.1.10",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "envify": "^3.4.0",
    "eslint": "^0.19.0",
    "eslint-plugin-react": "^2.1.1",
    "tape": "^4.0.0",
    "testling": "^1.7.1",
    "watchify": "^3.2.0"
  }
}
